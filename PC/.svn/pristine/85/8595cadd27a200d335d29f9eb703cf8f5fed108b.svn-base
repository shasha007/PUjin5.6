body{font-family: '微软雅黑 Light', '微软雅黑';}
.wk_container_regist{
	width: 100%;
	height: 820px;
	overflow: hidden;
	background-image: url(../images/u0.jpg);
	background-size: 100%;
	position: relative;
}
.wk_container_regist span{
	font-size: 32px;
	font-weight: bold;
	color: #fff;
	position: absolute;
	left: 50%;
	margin-left: -135px;
	top:85px;
}
.wk_regist{
	width: 410px;
	height: 300px;
	border-radius:5px ;
	background: #fff;
	margin: 182px auto;
	padding: 0px 55px;
	overflow: hidden;
}
.wk_regist p{
	width: 100%;
	overflow: hidden;
	font-size: 20px;
	font-weight: bold;
	color: #333;
	margin-top: 60px;
}
.wk_regist div{
	width: 100%;
	overflow: hidden;
	padding-bottom: 10px;
	border-bottom:1px solid #ccc;
}
.wk_regist div:after{
	content: ">";
	float: right;
}
/*王坤 end*/

