/*胡启文 start*/

/*公共头部*/
#PubilcHeader{
    width: 100%;
    border: none;
}
.h_PublicHeader{
    display: flex;
    width: 100%;
    max-width: 1080px;
	min-width: 720px;
    height: 88px;
    margin: 0 auto;
    line-height: 88px;
}
.h_PublicHeader div{
    flex: 1;
    margin: 0 15px;
}

.h_PuHe_btn{
    display: block;
    float: right;
    width: 88px;
    height: 32px;
    margin-top:  28px;
    line-height: 32px;
    text-align: center;
    color: #ea5504;
    border: 1px solid #ea5504;
    border-radius: 5px;
}

/*公共尾部*/
.h_PublicFooter{
	width: 100%;
	height: 186px;
	padding: 0 15px;
	color: #999;
	background-color: #f7f7f7;
}
.h_PublicFooter >div{
	display: flex;
	max-width: 1080px;
	min-width: 720px;
	margin: 0 auto;

}
.h_PublicFooter > div > div:nth-child(1){
